model Post {
  id        String    @id @default(uuid())
  title     String    @db.VarChar(50)
  body      String    @db.VarChar(500)
  users     Users     @relation(fields: [userId], references: [id])
  userId    String
  updatedAt DateTime  @updatedAt
  createdAt DateTime  @default(now())
  comments  Comment[]
  likes     Likes[]
  save      Save[]
}
